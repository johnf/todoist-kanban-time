{
  "name": "Todoist Kanban Time",
  "description": "Totals up task time on the kanban board",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
   {
     "matches": ["https://todoist.com/app/*"],
     "js": ["content.js"]
   }
 ],
 "icons": {
   "16": "/images/favicon_16.png",
   "32": "/images/favicon_32.png",
   "48": "/images/favicon_48.png",
   "128": "/images/favicon_128.png"
 }
}
